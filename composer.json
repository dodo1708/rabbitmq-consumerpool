{
  "name": "dodo1708/rabbitmq-consumerpool",
  "description": "A simple process pool consuming messages from a given rabbit mq instance on a single queue.",
  "keywords": [
    "rabbitmq",
    "consumer",
    "swoole",
    "openswoole",
    "process pool"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dominik Halle",
      "email": "dominikj.halle@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-sockets": "*",
    "monolog/monolog": "^2.3",
    "php-amqplib/php-amqplib": "^3.1",
    "php-di/php-di": "^6.3",
    "symfony/console": "^6.1"
  },
  "require-dev": {
    "jangregor/phpstan-prophecy": "^1.0.0",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.4",
    "squizlabs/php_codesniffer": "^3.6",
    "symfony/var-dumper": "^6.0"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "scripts": {},
  "provide": {
    "ext-redis": "*"
  }
}
